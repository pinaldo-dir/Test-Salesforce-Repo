    <description>HR Send Past Due Prob Reminder every first of month until Received Date is filled.</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>IF(
DAY( [HR_Probe_Report_Tracking__c].X1st_Probe_due_date__c +1 ) = 1,
[HR_Probe_Report_Tracking__c].X1st_Probe_due_date__c +1,
IF(
MONTH([HR_Probe_Report_Tracking__c].X1st_Probe_due_date__c +1) = 12,
DATE( YEAR([HR_Probe_Report_Tracking__c].X1st_Probe_due_date__c +1)+1, 1, 1 ),
DATE( YEAR([HR_Probe_Report_Tracking__c].X1st_Probe_due_date__c +1), MONTH ([HR_Probe_Report_Tracking__c].X1st_Probe_due_date__c +1) + 1, 1 )
)
)
</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_2_myRule_1_A1_8082900208</name>
        <dataType>Date</dataType>
        <expression>IF(
DAY( {!myVariable_current.X1st_Probe_due_date__c} +1 ) = 1,
{!myVariable_current.X1st_Probe_due_date__c} +1,
IF(
MONTH({!myVariable_current.X1st_Probe_due_date__c} +1) = 12,
DATE( YEAR({!myVariable_current.X1st_Probe_due_date__c} +1)+1, 1, 1 ),
DATE( YEAR({!myVariable_current.X1st_Probe_due_date__c} +1), MONTH ({!myVariable_current.X1st_Probe_due_date__c} +1) + 1, 1 )
)
)</expression>
    </formulas>
