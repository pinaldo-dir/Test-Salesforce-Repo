    <formulas>
        <description>Sets the mailing state based on the residential country when they are the same.</description>
        <name>flaMailingStateSameResi</name>
        <dataType>String</dataType>
        <expression>IF ( {!varSameMailing} &amp;&amp; {!varResiCountry} == &quot;United States of America&quot; , 
{!varResiState} , &quot;N/A&quot; )</expression>
    </formulas>
    <formulas>
        <description>Sets the Residential state based on the country</description>
        <name>flaResiState</name>
        <dataType>String</dataType>
        <expression>IF ( {!varResiCountry} == &apos;United States of America&apos; , {!varResiState} , &apos;N/A&apos; )</expression>
    </formulas>
