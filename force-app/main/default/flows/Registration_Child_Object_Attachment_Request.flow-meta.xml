    <description>Add record type to new attachments.</description>
    <environments>Default</environments>
    <formulas>
        <name>flaAttachmentInstructions</name>
        <dataType>String</dataType>
        <expression>{!objAttachmentBank.Instructions__c} + &apos; - &apos; + {!varDetail}</expression>
    </formulas>
    <formulas>
        <description>concatenates the name field with the detail passed from the child record</description>
        <name>flaAttachmentName</name>
        <dataType>String</dataType>
        <expression>{!objAttachmentBank.Attachment_Name__c} + &apos; - &apos; + {!varDetail}</expression>
    </formulas>
    <formulas>
        <name>flaAttachmentType</name>
        <dataType>String</dataType>
        <expression>TEXT ( {!objAttachmentBank.Registration_Document_Type__c} ) + &apos; - &apos; + {!varDetail}</expression>
    </formulas>
    <formulas>
        <name>flaOwnershipAttachmentName</name>
        <dataType>String</dataType>
        <expression>{!objAttachmentBank.Attachment_Name__c} + &apos; for owner - &apos; 
+ IF(!ISBLANK({!varFirstName}), {!varFirstName} + &apos; &apos;, &apos;&apos;) + IF( !ISBLANK({!varLastName}), {!varLastName}, &apos;&apos;)</expression>
    </formulas>
    <formulas>
        <description>adds the owners name to the instructions to make them clear</description>
        <name>flaOwnershipInstructions</name>
        <dataType>String</dataType>
        <expression>{!objAttachmentBank.Instructions__c} + &apos; for - &apos; 
+ IF(!ISBLANK({!varFirstName}), {!varFirstName} + &apos; &apos;, &apos;&apos;) + IF( !ISBLANK({!varLastName}), {!varLastName}, &apos;&apos;)</expression>
    </formulas>
