<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>49.0</apiVersion>
    <decisions>
        <name>Attachment_Needed</name>
        <label>Attachment Needed?</label>
        <locationX>806</locationX>
        <locationY>353</locationY>
        <defaultConnectorLabel>Not Needed</defaultConnectorLabel>
        <rules>
            <name>Request_Attachment</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varAttachmentIdentifier</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varAttachmentIdentifier</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varDateDeactivated</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Run_child_object_attachment_flow</targetReference>
            </connector>
            <label>Request Attachment</label>
        </rules>
    </decisions>
    <decisions>
        <name>isNew</name>
        <label>isNew</label>
        <locationX>269</locationX>
        <locationY>348</locationY>
        <defaultConnectorLabel>NotNew</defaultConnectorLabel>
        <rules>
            <name>New</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varIsNew</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_default_values</targetReference>
            </connector>
            <label>New</label>
        </rules>
    </decisions>
    <decisions>
        <name>Valid_Inputs</name>
        <label>Valid Inputs</label>
        <locationX>268</locationX>
        <locationY>159</locationY>
        <defaultConnectorLabel>Invalid</defaultConnectorLabel>
        <rules>
            <name>Valid</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varRegistrationID</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varRegistrationLocationID</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>isNew</targetReference>
            </connector>
            <label>Valid</label>
        </rules>
    </decisions>
    <description>Handles all actions related to the creation/update of a registration location
- added separator to flow to pass to sub flow</description>
    <formulas>
        <name>flaApplicantEmail</name>
        <dataType>String</dataType>
        <expression>BLANKVALUE ( {!varApplicantEmail} , &apos;&apos;)</expression>
    </formulas>
    <formulas>
        <description>overwrites the state if the country is not us</description>
        <name>flaState</name>
        <dataType>String</dataType>
        <expression>IF ( {!varCountry} = &apos;United States of America&apos;, {!varState} , &apos;N/A&apos;)</expression>
    </formulas>
    <interviewLabel>Registration Location Flow {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Registration Location Flow</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>Set_default_values</name>
        <label>Set default values</label>
        <locationX>551</locationX>
        <locationY>350</locationY>
        <connector>
            <targetReference>Attachment_Needed</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>varRegistrationLocationID</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>Applicant_Email__c</field>
            <value>
                <elementReference>flaApplicantEmail</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>State__c</field>
            <value>
                <elementReference>flaState</elementReference>
            </value>
        </inputAssignments>
        <object>Registration_Location__c</object>
    </recordUpdates>
    <start>
        <locationX>50</locationX>
        <locationY>50</locationY>
        <connector>
            <targetReference>Valid_Inputs</targetReference>
        </connector>
    </start>
    <status>Active</status>
    <subflows>
        <name>Run_child_object_attachment_flow</name>
        <label>Run child object attachment flow</label>
        <locationX>1094</locationX>
        <locationY>356</locationY>
        <flowName>Registration_Child_Object_Attachment_Request</flowName>
        <inputAssignments>
            <name>varObject</name>
            <value>
                <stringValue>Registration_Location__c</stringValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>varRegistrationID</name>
            <value>
                <elementReference>varRegistrationID</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>varAttachmentIdentifier</name>
            <value>
                <elementReference>varAttachmentIdentifier</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>varDetail</name>
            <value>
                <elementReference>varAttachmentDetail</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>varSourceRecordID</name>
            <value>
                <elementReference>varRegistrationLocationID</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>varSeparator</name>
            <value>
                <elementReference>varSeparator</elementReference>
            </value>
        </inputAssignments>
    </subflows>
