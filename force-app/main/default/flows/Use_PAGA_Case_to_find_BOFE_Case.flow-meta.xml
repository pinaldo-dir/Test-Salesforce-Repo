            <field>Employer__c</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>PAGACaseSObject.Id</assignToReference>
            <field>Id</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>PAGACaseSObject.Related_BOFE_Case__c</assignToReference>
