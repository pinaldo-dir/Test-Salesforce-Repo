<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Interest_Amt__c</fullName>
    <description>This field will allow interest to be summarized in its own column on  a report.</description>
