<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Appointment_Type_Required_Field</fullName>
    <active>true</active>
    <description>Appointment Type is a required field</description>
    <errorConditionFormula>NOT( Start_Date__c  &lt;  DATEVALUE("2016-05-18"))
&amp;&amp; ISBLANK(TEXT(Type__c))</errorConditionFormula>
    <errorMessage>Please fill in Appointment Type field</errorMessage>
</ValidationRule>
