<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Active_Relationship_Reason_checker</fullName>
    <active>true</active>
    <errorConditionFormula>AND( Relationship_Status__c = true ,  Reason__c  &lt;&gt; '')</errorConditionFormula>
    <errorDisplayField>Reason__c</errorDisplayField>
    <errorMessage>Reason not required for active relationship.</errorMessage>
</ValidationRule>
