<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Wage_Adjustment_Amount__c</fullName>
    <description>Calculates the adjustment amount of the violation when an adjustment is made via a hearing</description>
    <externalId>false</externalId>
    <formula>IF ( Original_Wage_Assessment_Amount__c &gt; 0, 
     Wages_Due__c - Original_Wage_Assessment_Amount__c,
     null )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>"EE(s) Amount Assessed"  â€”  "Original Amount Payable to EE(s)"</inlineHelpText>
    <label>Wage Adjustment Amount</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
