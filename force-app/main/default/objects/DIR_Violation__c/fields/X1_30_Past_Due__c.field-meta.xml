<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>X1_30_Past_Due__c</fullName>
    <description>1-30 day past due penalty amount</description>
    <externalId>false</externalId>
    <formula>IF ( ( ( ( TODAY() -  Payment_Due_Date__c ) &gt; 30 ) &amp;&amp; ( TODAY() -  Payment_Due_Date__c ) &lt;= 60 )  ,  Penalty_Balance_Due__c , 0 )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>1 - 30 Past Due</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
