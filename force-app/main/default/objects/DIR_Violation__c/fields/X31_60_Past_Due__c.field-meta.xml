<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>X31_60_Past_Due__c</fullName>
    <description>31 - 60 days past due - penalties only</description>
    <externalId>false</externalId>
    <formula>IF ( ( ( ( TODAY() -  Payment_Due_Date__c ) &gt; 60 ) &amp;&amp; ( TODAY() -  Payment_Due_Date__c ) &lt;= 90 )  ,  Penalty_Balance_Due__c , 0 )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>31 - 60 Past Due</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
