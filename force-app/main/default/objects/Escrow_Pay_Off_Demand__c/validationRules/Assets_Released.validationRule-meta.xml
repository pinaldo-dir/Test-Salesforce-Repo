<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Assets_Released</fullName>
    <active>true</active>
    <description>Assets released should be refered if Partial Release = True</description>
    <errorConditionFormula>ISBLANK(Assets_Released__c) &amp;&amp; Partial_Release__c</errorConditionFormula>
    <errorDisplayField>Assets_Released__c</errorDisplayField>
    <errorMessage>Asset released should be referenced.</errorMessage>
</ValidationRule>
