<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Number_of_Days_to_Resolution__c</fullName>
    <description>Formula to calculate difference between the Date of Docket and Resolution Date on a Case.</description>
    <externalId>false</externalId>
    <formula>( ( Case__r.Resolution_Date__c ) - ( Case__r.Date_of_Docket__c ) )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Number of Days to Resolution</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
