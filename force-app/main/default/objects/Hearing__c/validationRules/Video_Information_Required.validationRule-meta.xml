<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Video_Information_Required</fullName>
    <active>true</active>
    <errorConditionFormula>OR(AND( NOT(ISBLANK(Video_Meeting_URL__c)),  ISBLANK(Video_Meeting_Information__c) ),AND( NOT(ISBLANK(Video_Meeting_Information__c)),  ISBLANK(Video_Meeting_URL__c) ))</errorConditionFormula>
    <errorMessage>Video Meeting URL and Video Meeting ID and Passcode is required.</errorMessage>
</ValidationRule>
