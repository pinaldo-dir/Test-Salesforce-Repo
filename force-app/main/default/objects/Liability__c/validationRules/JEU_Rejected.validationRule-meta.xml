<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>JEU_Rejected</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    TEXT(Refer_to_JEU__c) = 'Rejected',
    ISBLANK( JEU_Reject_Reason__c)
)</errorConditionFormula>
    <errorMessage>JEU Rejection Reason is required</errorMessage>
</ValidationRule>
