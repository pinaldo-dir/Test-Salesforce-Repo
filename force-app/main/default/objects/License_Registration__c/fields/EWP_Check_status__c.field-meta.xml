<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EWP_Check_status__c</fullName>
    <externalId>false</externalId>
    <formula>IF((RecordType.DeveloperName  == 'Entertainment_Work'  &amp;&amp;   (ISPICKVAL(Status__c, 'Pending / Defect') || ISPICKVAL(Status__c, 'Submitted') || ISPICKVAL(Status__c, 'Resubmitted') || ( ISPICKVAL(Service_Type__c, 'Renewal')))), 'True', 'False')</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>EWP Check status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
