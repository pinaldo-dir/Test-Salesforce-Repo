<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_or_License_Requred</fullName>
    <active>true</active>
    <errorConditionFormula>AND(RecordType.DeveloperName = 'License', 
ISBLANK(Account__c),
ISBLANK(License__c))</errorConditionFormula>
    <errorMessage>You must have an Account or another Registration selected.</errorMessage>
</ValidationRule>
