<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_Received</fullName>
    <active>true</active>
    <description>Date Received cannot be in the future</description>
    <errorConditionFormula>Date_Received__c &gt; today()</errorConditionFormula>
    <errorDisplayField>Date_Received__c</errorDisplayField>
    <errorMessage>Date Received cannot be in the future</errorMessage>
</ValidationRule>
