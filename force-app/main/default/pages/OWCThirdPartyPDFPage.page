            <div>
                <span style="width:100%;">
                    <span class="lightgray" style="font-family:Arial Unicode MS, Helvetica, sans-serif; font-size:11px">
                        []&nbsp;{!$Label.owc_reported_to_work}
                    </span>
                </span>
            </div>
            <apex:repeat value="{!wageDefDetails[0].payClaimDetails}" var="payClaimDetailsObj" id="theRepeat214">
                <div>
                    <span style="width:100%;">
                        <strong style="font-family:Arial, Helvetica, sans-serif; font-size:11px"> {!$Label.OWC_reporting_time_pay_claim_heading}</strong>
                        <span class="lightgray" style="font-family:Arial, Helvetica, sans-serif; font-size:11px">{!$Label.OWC_one_hourly_rate}</span>
                    </span>
                </div>
                <div style="display:{!IF(payClaimDetailsObj.isOneHourlyRatePOW == true, 'block','none')}">
                    <apex:repeat value="{!payClaimDetailsObj.violationTypeVariablesForOneHourlyRate}" var="oneHourlyRate" id="theRepeat29">
                        <div style="display:{!IF((LEN(oneHourlyRate.value) > 0), 'block','none')}">
                            <span style="width:100%;">
                                <strong style="font-family:Arial, Helvetica, sans-serif; font-size:11px">{!oneHourlyRate.label}</strong>
                                <span class="lightgray"  style="display:{!IF(oneHourlyRate.dataType == 'Currency','inline','none')}; font-family:Arial, Helvetica, sans-serif; font-size:11px">$</span>
                                <span class="lightgray" style="font-family:Arial, Helvetica, sans-serif; font-size:11px">{!oneHourlyRate.value}</span>
                            </span>
                        </div>
                    </apex:repeat>
                </div>
            </apex:repeat>
        </div>
        <!--any other claims you would like to make that are not listed here OutputPanel Section-->
        <div style="display:{!IF(wageDefDetails[0].anotherClaimExplain != null, 'block','none')}; margin-top: 10px;" cellpadding="0" cellspacing="0">
            <div style="display:{!IF(wageDefDetails[0].anotherClaimExplain != null, 'block','none')}">
                <span style="width:100%;">
                    <strong style="font-family:Arial, Helvetica, sans-serif; font-size:11px">{!$Label.OWC_wagedef_List_Claims}</strong>
                    <span class="lightgray" style="font-family:Arial, Helvetica, sans-serif; font-size:11px">{!wageDefDetails[0].anotherClaimExplain}</span>
                </span>
            </div>
        </div>
        <!-------------------------------------Wage Deficiency Section End------------------------------ -->
        <br/>
        <!-----------------------------------Certificate And Authorization Start Here---------------------------- -->
        <div style="width: 100%; background-color: #d8d8d8;" cellpadding="0" cellspacing="0">
            <div>
                <span style="width:100%;padding-top:2px;padding-bottom:3px;">
                    <strong style="font-family:Arial, Helvetica, sans-serif; font-size:11px">
                        {!$Label.OWC_certification_section_heading}
                    </strong>
                </span>
            </div>
        </div>
        <br/>
        <!--List Of Files -->
        <div cellspacing="0" cellpadding="0" class="c_table" width='100%' style="display:{!IF(CertificationAndAuthObj.certificationDocs != null, 'block','none')}">
            <div style="page-break-inside:avoid;">
                <span>
                    <strong style="font-family:Arial, Helvetica, sans-serif; font-size:11px">{!$Label.OWC_file_name_header_label}</strong>
                </span>
            </div>
            <apex:repeat value="{!CertificationAndAuthObj.certificationDocs}" var="item">
                <div style="page-break-inside:avoid;">
                    <span>
                        <strong class="lightgray" style="font-family:Arial, Helvetica, sans-serif; font-size:11px">{!item}</strong>
                    </span>
                </div>
            </apex:repeat>
        </div>
        <!-----------------------------------Certificate And Authorization End Here---------------------------- -->
        <div cellspacing="0" cellpadding="0" width='100%'>
            <div>
                <span style="width:100%;">
                    <strong style="font-family:Arial, Helvetica, sans-serif; font-size:11px">{!$Label.OWC_Certification_Statement}</strong>
                    <span style="font-family:Arial, Helvetica, sans-serif; font-size:11px; color:#3b3838">
                        {!$label.OWC_certification_authorization_to_release}&nbsp;
                        <a href="www.dir.ca.gov/od_pub/privacy.html" target="_blank">www.dir.ca.gov/od_pub/privacy.html</a>. </span>
                    <span style="padding: 20px 0 0 0px;"></span>
                </span>
                </div>
        </div>
        <!----------------------------------Declaration Person Start Here--------------------------- -->
        <div cellspacing="0" cellpadding="0" width='100%'>
            <div style="display:{!IF(industryInfoWrapperObj.listOfDuties != null, 'block','none')}">
                <span style="width:100%;">
                    <strong style="font-family:Arial, Helvetica, sans-serif; font-size:11px">{!$Label.OWC_authorised_claimant_date}</strong>
                    <span class="lightgray" style="font-family:Arial, Helvetica, sans-serif; font-size:11px">{!CertificationAndAuthObj.claimDate}</span>
                </span>
            </div>
            <div style="display:{!IF(CertificationAndAuthObj.claimantName != null, 'block','none')}">
                <span style="width:100%;">
                    <strong style="font-family:Arial, Helvetica, sans-serif; font-size:11px">{!$Label.OWC_authorised_claimant_name}:</strong>
                    <span class="lightgray" style="font-family:Arial, Helvetica, sans-serif; font-size:11px">{!CertificationAndAuthObj.claimantName}</span>
                </span>
            </div>
        </div>
        <!----------------------------------Declaration Person End Here--------------------------- -->



