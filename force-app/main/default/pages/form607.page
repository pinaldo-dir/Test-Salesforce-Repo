<body>
    <apex:form >
        <apex:pageBlock >
            <p style="text-indent: 0pt; line-height: 5pt; text-align: left;">
                <br />
            </p>

            <apex:pageMessages rendered="{!renderMode==''}" />
            <apex:outputPanel style="padding-left:350px"
                rendered="{!form607.Id!=null&&renderMode!='PDF'}">
                <apex:commandButton value="Delete" action="{!deleteForm607}" />
                <apex:commandButton rendered="{!approvalStatus=='Not Submitted'}"
                    value="Submit for Approval" action="{!submitForApproval}" />
                <apex:commandButton rendered="{!(numberOfpositions_5d!=''&&numberOfpositions_5d!='1')||(numberOfpositions_8d!=''&&numberOfpositions_8d!='1')}"
                    value="Edit Posiiton Modifications"
                    action="{!addPositionModifications}" />
                <apex:commandButton value="Print PDF"
                    onClick="window.open('{!pdfURL}')" />
                <apex:commandButton rendered="{!(numberOfpositions_5d!=''&&numberOfpositions_5d!='1')||(numberOfpositions_8d!=''&&numberOfpositions_8d!='1')}"
                    value="View Attachment" onClick="window.open('{!attachmentURL}')" />
                <apex:commandButton value="Detail Page" action="{!detailPage}" />
                <br />
                <apex:outputText style="font-size:18pt;font-family:sans-serif;font-weight:bold"
                    value="{!pageTitle}" />
                <br />
                <apex:outputText style="font-size:14pt;font-family:sans-serif"
                    value="{!'Approval Status: ' + approvalStatus}" />
            </apex:outputPanel>
            <table style="border-collapse: collapse; width: 100%">
                <tr>
                    <td
                        style="width: 17%; border-top-style: solid; border-top-width: 1pt; border-bottom-style: solid; border-bottom-width: 1pt; border-right-style: solid; border-right-width: 1pt"
                        rowspan="2">
                        <p class="s1"
                            style="margin-top: 0px; padding-left: 20px; text-indent: 0pt; text-align: left;">
                            STD. 607 (REV. 10/2008)</p>
                        <p style="padding-left: 20px; text-indent: 0pt; text-align: left;">
                            <br />
                        </p>
                    </td>
                    <td
                        style="width: 68%; border-top-style: solid; border-top-width: 1pt; border-left-style: solid; border-left-width: 1pt; border-bottom-style: solid; border-bottom-width: 1pt; border-right-style: solid; border-right-width: 1pt"
                        rowspan="3">
                        <p class="s2"
                            style="text-indent: 0pt; line-height: 11pt; text-align: center;">
                            STATE OF
                            CALIFORNIA&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;PAYROLL
                            ROSTER CHANGE</p>
                        <p class="s3" style="text-indent: 0pt; text-align: center;">
                            CHANGE IN ESTABLISHED POSITIONS</p>
                    </td>
                    <td
                        style="border-top-style: solid; border-top-width: 1pt; border-left-style: solid; border-left-width: 1pt; border-bottom-style: solid; border-bottom-width: 1pt"
                        rowspan="2">
                        <p class="s4"
                            style="padding-left: 32px; margin-top: 0px; padding-top: 0px; text-indent: 0pt; text-align: left;{!pdfFontSize}">
                            ROUTING<br />
                        </p>
                        <div class="output">
                            <apex:inputText rendered="{!renderMode==''}" value="{!routing}"
                                maxlength="10" size="10" />
                            <apex:outputPanel rendered="{!routing==''&&renderMode=='PDF'}">
                                <br />
                            </apex:outputPanel>
                            <apex:outputText rendered="{!routing!=''&&renderMode=='PDF'}"
                                value="{!routing}" />
                        </div>
                    </td>
                </tr>
                <tr></tr>
                <tr>
                    <td
                        style="border-top-style: solid; border-top-width: 1pt; border-bottom-style: solid; border-bottom-width: 1pt; border-right-style: solid; border-right-width: 1pt"
                        rowspan="4">
                        <p class="s4"
                            style="padding-left: 5pt; text-indent: 0pt; line-height: 10pt; text-align: left;{!pdfFontSize}">
                            1. ROUTE TO:<br />
                            <apex:selectRadio rendered="{!renderMode==''}" disabled="true"
                                value="{!routeTo}">
                                <apex:selectOptions value="{!routeToOptions}" />
                            </apex:selectRadio>

                            <!--<apex:image rendered="{!renderMode=='PDF'}" height="25" value="{!URLFOR($Resource.boxImages, 'controllerDivisionChecked.png')}"/>-->
                            <apex:image rendered="{!renderMode=='PDF'}" height="25"
                                value="{!URLFOR($Resource.boxImages, 'controllerDivisionUnchecked.png')}" />
                            <br />
                            <apex:image rendered="{!renderMode=='PDF'}" height="20"
                                value="{!URLFOR($Resource.boxImages, 'routeToOtherUnchecked.png')}" />
                            <!--<apex:image rendered="{!routeTo!='true'&&renderMode=='PDF'}" height="20" value="{!URLFOR($Resource.boxImages, 'routeToOtherChecked.png')}"/>-->
                        </p>
                    </td>
                    <td
                        style="border-top-style: solid; border-top-width: 1pt; border-left-style: solid; border-left-width: 1pt; border-bottom-style: solid; border-bottom-width: 1pt"
                        rowspan="2">
                        <p class="s4"
                            style="padding-left: 5pt; text-indent: 0pt; line-height: 7pt; text-align: left;{!pdfFontSize}">
                            3 (a) Fiscal Year<br />
                        </p>
                        <div class="output">
                            <apex:inputText rendered="{!renderMode==''}"
                                value="{!fiscalYear}" maxlength="10" size="10" />
                            <apex:outputPanel rendered="{!fiscalYear==''&&renderMode=='PDF'}">
                                <br />
                            </apex:outputPanel>
                            <apex:outputText rendered="{!fiscalYear!=''&&renderMode=='PDF'}"
                                value="{!fiscalYear}" />
                        </div>
                    </td>
                </tr>
                <tr>
                    <td
                        style="border-top-style: solid; border-top-width: 1pt; border-left-style: solid; border-left-width: 1pt; border-right-style: solid; border-right-width: 1pt"
                        rowspan="3">
                        <p class="s1"
                            style="padding-left: 3pt; text-indent: 0pt; line-height: 7pt; text-align: left;{!pdfFontSize}">
                            2. Type of Transaction</p> <apex:selectRadio rendered="{!renderMode==''}" value="{!transactionType}">
                            <apex:selectOptions value="{!transactionOptions}" />
                        </apex:selectRadio> <apex:image rendered="{!transactionType!='Proposed New'&&renderMode=='PDF'}"
                            height="30"
                            value="{!URLFOR($Resource.boxImages, 'proposedNewPositionUnchecked.png')}" />
                        <apex:image rendered="{!transactionType=='Proposed New'&&renderMode=='PDF'}"
                            height="30"
                            value="{!URLFOR($Resource.boxImages, 'proposedNewPositionChecked.png')}" />&nbsp;&nbsp;&nbsp;
                        <apex:image rendered="{!transactionType!='Extension of Present'&&renderMode=='PDF'}"
                            height="30"
                            value="{!URLFOR($Resource.boxImages, 'extensionOfPresentPositionUnchecked.png')}" />
                        <apex:image rendered="{!transactionType=='Extension of Present'&&renderMode=='PDF'}"
                            height="30"
                            value="{!URLFOR($Resource.boxImages, 'extensionOfPresentPositionChecked.png')}" />&nbsp;&nbsp;&nbsp;
                        <apex:image rendered="{!transactionType!='Reclassification'&&renderMode=='PDF'}"
                            height="30"
                            value="{!URLFOR($Resource.boxImages, 'reclassificationOfPositionUnchecked.png')}" />
                        <apex:image rendered="{!transactionType=='Reclassification'&&renderMode=='PDF'}"
                            height="30"
                            value="{!URLFOR($Resource.boxImages, 'reclassificationOfPositionChecked.png')}" />&nbsp;&nbsp;&nbsp;
                        <apex:image rendered="{!transactionType!='Encumbrance'&&renderMode=='PDF'}"
                            height="30"
                            value="{!URLFOR($Resource.boxImages, 'encumbranceIncreaseUnchecked.png')}" />
                        <apex:image rendered="{!transactionType=='Encumbrance'&&renderMode=='PDF'}"
                            height="30"
                            value="{!URLFOR($Resource.boxImages, 'encumbranceIncreaseChecked.png')}" />&nbsp;&nbsp;&nbsp;
                        <apex:image rendered="{!transactionType!='Abolishment'&&renderMode=='PDF'}"
                            height="30"
                            value="{!URLFOR($Resource.boxImages, 'abolishmentOfPositionUnchecked.png')}" />
                        <apex:image rendered="{!transactionType=='Abolishment'&&renderMode=='PDF'}"
                            height="30"
                            value="{!URLFOR($Resource.boxImages, 'abolishmentOfPositionChecked.png')}" />&nbsp;&nbsp;&nbsp;
                        <apex:image rendered="{!transactionType!='Disencumbrance'&&renderMode=='PDF'}"
                            height="30"
                            value="{!URLFOR($Resource.boxImages, 'disencumbranceUnchecked.png')}" />
                        <apex:image rendered="{!transactionType=='Disencumbrance'&&renderMode=='PDF'}"
                            height="30"
                            value="{!URLFOR($Resource.boxImages, 'disencumbranceChecked.png')}" />&nbsp;&nbsp;&nbsp;
                        <apex:image rendered="{!transactionType!='Other'&&renderMode=='PDF'}"
                            height="30"
                            value="{!URLFOR($Resource.boxImages, 'otherUnchecked.png')}" /> <apex:image rendered="{!transactionType=='Other'&&renderMode=='PDF'}"
                            height="30"
                            value="{!URLFOR($Resource.boxImages, 'otherChecked.png')}" />
                    </td>
                </tr>
                <tr>
                    <td
                        style="border-top-style: solid; border-top-width: 1pt; border-left-style: solid; border-left-width: 1pt; border-bottom-style: solid; border-bottom-width: 1pt"
                        rowspan="2">
                        <p class="s4"
                            style="padding-left: 5pt; text-indent: 0pt; text-align: left;{!pdfFontSize}">
                            3 (b) Document No.<br />
                        </p>
                        <div class="output">
                            <apex:inputText rendered="{!renderMode==''}"
                                value="{!documentNo}" maxlength="10" size="10" />
                            <apex:outputPanel rendered="{!documentNo==''&&renderMode=='PDF'}">
                                <br />
                            </apex:outputPanel>
                            <apex:outputText rendered="{!documentNo!=''&&renderMode=='PDF'}"
                                value="{!documentNo}" />
                        </div>
                    </td>
                </tr>
                <tr></tr>
            </table>
            <table style="border-collapse: collapse; width: 100%">
                <tr>
                    <td
