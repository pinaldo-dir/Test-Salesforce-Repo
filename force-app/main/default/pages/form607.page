                        <div class="output">
                            <apex:inputText rendered="{!renderMode==''}"
                                value="{!expirationDate_4b}" maxlength="10" size="10" />
                            <apex:outputPanel rendered="{!expirationDate_4b==''&&renderMode=='PDF'}">
                                <br />
                            </apex:outputPanel>
                            <apex:outputText rendered="{!expirationDate_4b!=''&&renderMode=='PDF'}"
                                value="{!expirationDate_4b}" />
                        </div>
                    </td>
                    <td
                        style="border-left-style: solid; border-left-width: 1pt; border-right-style: solid; border-right-width: 1pt" />
                    <td
                        style="width: 25%; border-left-style: solid; border-left-width: 1pt; border-bottom-style: solid; border-bottom-width: 1pt; border-right-style: solid; border-right-width: 1pt">
                        <apex:outputPanel styleClass="output"
                            rendered="{!renderMode==''&&WFD_effectiveDate_7a==false}">
                            <apex:outputText style="color:red" value="{!dateFormatError}" />
                            <br />
                        </apex:outputPanel>
                        <div class="lineup"
                            style="padding-left: 5pt; text-indent: 0pt; text-align: left;{!pdfFontSize}">
                            7 (a) Effective Date&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</div>
                        <div class="output">
                            <apex:inputText rendered="{!renderMode==''}"
                                value="{!effectiveDate_7a}" maxlength="10" size="10" />
                            <apex:outputPanel rendered="{!effectiveDate_7a==''&&renderMode=='PDF'}">
                                <br />
                            </apex:outputPanel>
                            <apex:outputText rendered="{!effectiveDate_7a!=''&&renderMode=='PDF'}"
                                value="{!effectiveDate_7a}" />
                        </div>
                    </td>
                    <td
                        style="width: 25%; border-left-style: solid; border-left-width: 1pt; border-bottom-style: solid; border-bottom-width: 1pt">
                        <apex:outputPanel styleClass="output"
                            rendered="{!renderMode==''&&WFD_disencumberTo_7b==false}">
                            <apex:outputText style="color:red" value="{!dateFormatError}" />
                            <br />
                        </apex:outputPanel>
                        <div class="lineup"
                            style="padding-left: 5pt; text-indent: 0pt; text-align: left;{!pdfFontSize}">
                            7 (b) Disencumber To&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</div>
                        <div class="output">
                            <apex:inputText rendered="{!renderMode==''}"
                                value="{!disencumberTo_7b}" maxlength="10" size="10" />
                            <apex:outputPanel rendered="{!disencumberTo_7b==''&&renderMode=='PDF'}">
                                <br />
                            </apex:outputPanel>
                            <apex:outputText rendered="{!disencumberTo_7b!=''&&renderMode=='PDF'}"
                                value="{!disencumberTo_7b}" />
                        </div>
                    </td>
                </tr>
            </table>

            <!-- <table style="border-collapse:collapse;width:574pt;margin-left:5.96398pt">-->
