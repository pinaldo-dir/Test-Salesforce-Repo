    <rules>
        <fullName>BOFE Referral Non-response</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Referral__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>BOFE Request for Legal Services,BOFE Request for Legal Court Action,BOFE Civil Litigation Referral</value>
        </criteriaItems>
        <criteriaItems>
            <field>Referral__c.Request_Status__c</field>
            <operation>notEqual</operation>
            <value>Pending Approval,Denied,Review in Process,Closed</value>
        </criteriaItems>
        <description>Actions to take when an referral has not been responded to as expected</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Non_Response_notificatino</name>
                <type>Alert</type>
            </actions>
            <timeLength>30</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>BOFE Referral Status Change</fullName>
        <actions>
            <name>Referral_status_outcome_notification</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>Rule fires when the status changes on a referral from a BOFE</description>
        <formula>(  RecordType.DeveloperName =    'BOFE_Civil_Litigation_Referral'  ||  RecordType.DeveloperName =    'BOFE_Request_for_Legal_Court_Action'  ||  RecordType.DeveloperName =    'BOFE_Request_for_Legal_Services' ) &amp;&amp; (   ISCHANGED ( Request_Status__c )   ||   ISCHANGED ( Outcome__c ) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>RCI Referral - WJC Rejected</fullName>
        <actions>
            <name>Judgment_Referral_Rejected_by_WJC</name>
            <type>Task</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Referral__c.RecordTypeId</field>
            <operation>contains</operation>
            <value>RCI</value>
        </criteriaItems>
        <criteriaItems>
            <field>Referral__c.Wage_Justice_Center_Disposition__c</field>
            <operation>equals</operation>
            <value>Rejected</value>
        </criteriaItems>
        <criteriaItems>
            <field>Referral__c.Referred_to_Wage_Justice_Center__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>DIR_Case__c.RecordTypeId</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>DIR_Case__c.Closed__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>RCI Referral Approved</fullName>
        <actions>
            <name>Email_Assigned_Deputy</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Update_Referral_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Referral__c.Request_Status__c</field>
            <operation>equals</operation>
            <value>Request Approved,Referred</value>
        </criteriaItems>
        <criteriaItems>
            <field>Referral__c.RecordTypeId</field>
            <operation>contains</operation>
            <value>RCI</value>
        </criteriaItems>
        <criteriaItems>
            <field>DIR_Case__c.Status__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>RCI Referral for Mediation</fullName>
        <actions>
            <name>No_Activity_on_Mediation_Referral_in_120_Days</name>
            <type>Task</type>
        </actions>
        <active>true</active>
        <description>Send notification 120 days from Last Activity Date</description>
        <formula>(RecordType.DeveloperName = "RCI Referral to Mediation") &amp;&amp; LastActivityDate &gt; DATEVALUE(CreatedDate+1)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>RCI Update Legal Referral Count</fullName>
        <actions>
            <name>Update_Legal_Referral_Field</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Referral__c.RCILegalReferral__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Referral WCA 45 Day Inactivity Notification</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Referral__c.Request_Status__c</field>
            <operation>notEqual</operation>
            <value>Court Decision,Settled,Denied,Closed</value>
        </criteriaItems>
        <criteriaItems>
            <field>Referral__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>WCA Legal Opinion,WCA Request for Legal Action</value>
        </criteriaItems>
        <description>Sends email notification when legal opinion has not been updated in 45 day.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <offsetFromField>Referral__c.LastModifiedDate</offsetFromField>
            <timeLength>45</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Referral%3A Notify PAGA Investigator 30 days before Investigation deadlline</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Referral__c.Investigation_Deadline_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>DIR_Case__c.Status__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <description>Notify PAGA investigator 30 days before the Investigation Deadline Date</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>X30_Day_Notification_before_Investigation_Deadline</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Referral__c.Investigation_Deadline_Date__c</offsetFromField>
            <timeLength>-30</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
